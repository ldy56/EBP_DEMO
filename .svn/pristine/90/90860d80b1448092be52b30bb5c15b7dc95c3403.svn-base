.nav>a:nth-of-type(1){margin: 0px 45px 0px 50px;}
.nav>a:nth-of-type(2){margin-right: 40px;color: #E80F3C;}
.nav>a:nth-of-type(3){margin-right: 35px;}
.nav>a:nth-of-type(4){margin-right: 40px;}
#adv{width:100%;height: 38px; background: #FAFAFA; border: 1px solid #cccccc}
#adv>div{font-weight: 600;color: #808080;width:1200px;height: 38px;line-height: 38px; margin: 0 auto}
#adv>div>a{color: #808080}
#adv>div>span{font-weight: 400}
#shopping{width: 1200px;height: auto;margin: 40px auto}
.data{font-weight: 600;font-size: 19px;}
.data>input{font-size: 19px; }
.data>input:nth-of-type(1){height: 35px;width: 156px ;border-radius: 3px;border: 1px solid #cccccc}
.data>a{border:1px solid #cccccc;background: -webkit-linear-gradient(#EEEEEE,#DADADA); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(#EEEEEE,#DADADA); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(#EEEEEE,#DADADA); /* Firefox 3.6 - 15 */
    background: linear-gradient(#EEEEEE,#DADADA);
    display: inline-block;text-align: center;line-height: 40px;color:black ;
    /* 标准的语法 */height: 38px;width: 94px;border-radius: 6px;margin-left: 20px}
.content{width: 1300px;height: auto;margin: 0 auto;}
.content>ul>li{float: left;width:600px;height:298px; }
.content>ul>li:nth-of-type(3),.content>ul>li:nth-of-type(4){
    margin: 47px 0;
}
.content>ul>li:nth-of-type(1),.content>ul>li:nth-of-type(3){
    margin-right:50px;margin-left: 50px;
}
.content_left{  float: left;}
.content_left>img{border: 1px solid #cccccc}
.content_right{  float: left;margin-left: 30px;position: relative}
.content_right_img>img:nth-of-type(2){margin-left: 89px}
.content_right_img>img:nth-of-type(3){margin-left: 90px}
.line_red{width:90px;height: 4px;background: #ED0E3B;position: absolute;top: 79px;}
.line_hui{width:90px;height: 4px;background:#D8D8D8;position: absolute;top: 79px;}
.line_rht{left: 14px}
.line_cnt{left: 121px}
.line_lft{left: 225px}
.content_right>span{font-weight: 600;font-size: 18px;color: #828282}
.content_right>span:nth-of-type(1){padding-right: 22px;}
.content_right>span:nth-of-type(2){padding-right: 55px}
#color_red{color: #F30E35}
#color_red2{color: #F30E35}
#color_red3{color: #F30E35}
#color_red4{color: #F30E35}
.content_right_img{margin-top: 3px}
.palytime{
    width:302px ;
    height: 40px;
    line-height: 40px;
    color: #828282;
    border: 1px solid #cccccc;
    padding-left: 10px;
    margin-top: 15px;
}
.price{margin-top: 10px}
.price>span{font-weight: 600;font-size: 23px}
#price_size{font-size: 50px;color: #E80F3C}
#price_size2{font-size: 50px;color: #E80F3C}
#price_size3{font-size: 50px;color: #E80F3C}
#price_size4{font-size: 50px;color: #E80F3C}
.price_color{color: #E80F3C}
.some{margin-top: 10px}
.some>input:nth-of-type(1){width: 78px;height: 32px;background: #FCFCFC;border: 1px solid #cccccc}
.some>input:nth-of-type(2){cursor: pointer;width: 100px;height: 32px;border: 1px solid #E80F3C;background: #E80F3C;margin-left: 20px;color: white;font-weight: 600;font-size: 16px}
.save{color: #7D7D7D;font-size: 18px;margin-top: 10px;}