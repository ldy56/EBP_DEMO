.nav>a:nth-of-type(1){margin: 0px 45px 0px 50px;}
.nav>a:nth-of-type(2){margin-right: 40px;}
.nav>a:nth-of-type(3){margin-right: 35px;}
.nav>a:nth-of-type(4){margin-right: 40px;}
#adv{width:100%;height: 38px; background: #FAFAFA; border: 1px solid #cccccc}
#adv>div{font-weight: 600;color: #808080;width:1200px;height: 38px;line-height: 38px; margin: 0 auto}
#adv>div>a{color: #808080}
#adv>div>span{font-weight: 400}
#content{width: 1200px;height: auto;margin: 25px auto 0px;}
.idmessage{width: 1200px;height:495px;
    background-color: #F5F5F5; padding-top: 50px}
.id_left{width: 700px;  height:400px;margin:0px auto;padding-top: 32px;background: #74B6CC;border-radius: 20px}
.id_left_bj{width: 345px;  height:300px;margin:0px auto;padding-top: 32px;}
.pic{width: 345px;height:126px;text-align: center}

.adminm{width: 189px;height: auto;float:right;font-size: 18px;font-weight: 600}
.adminm>p:nth-of-type(2),.adminm>p:nth-of-type(3),.adminm>p:nth-of-type(4){margin-top:10px; }
.adminm>p>span{color: red}
.adminm>div{margin-top: 10px}
.adminm>div>input{cursor: pointer;font-weight: 600;height: 25px; background: -webkit-linear-gradient(#FB3738,#D70101); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(#FB3738,#D70101); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(#FB3738,#D70101); /* Firefox 3.6 - 15 */
    background: linear-gradient(#FB3738,#D70101); /* 标准的语法 */
    border-radius: 6px;color: white;border: none;font-size: 18px;}
.adminm>div>input:nth-of-type(1){width:74px;}
.adminm>div>input:nth-of-type(2){width:98px;margin-left: 15px}
.passwd{font-weight: 600;font-size: 18px;margin-top: 40px}
.inpt>input{font-size: 18px;width: 238px;height: 30px;border-radius: 6px;border: 1px solid #cccccc;}
.input1{cursor: pointer;width:74px;font-weight: 600;height: 25px;background: -webkit-linear-gradient(#FB3738,#D70101); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(#FB3738,#D70101); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(#FB3738,#D70101); /* Firefox 3.6 - 15 */
    background: linear-gradient(#FB3738,#D70101); /* 标准的语法 */
    border-radius: 6px;color: white;border: none;font-size: 18px;}
#forget{margin-left: 90px}

.btnc{
    background: -webkit-linear-gradient(#FB3738,#D70101); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(#FB3738,#D70101); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(#FB3738,#D70101); /* Firefox 3.6 - 15 */
    background: linear-gradient(#FB3738,#D70101); /* 标准的语法 */
    border-radius: 6px;color: white;
}
