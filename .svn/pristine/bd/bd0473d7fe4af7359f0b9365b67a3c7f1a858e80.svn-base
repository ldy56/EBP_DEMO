.td_font{color:#D70504;}
.butn{width: 500px;height: 45px;position: absolute;left: 210px;bottom: 40px;}
.butn>ul>li{float: left;margin: 0px 10px;text-align: center}
.btn_ul>li:nth-of-type(1)>input, .btn_ul>li:nth-of-type(7)>input{width: 66px;height: 26px;font-weight: 600}
.btn_ul a{
    color: black; font-weight: 600;line-height: 28px}
.btn_ul>li:nth-of-type(8)>input{width:41px;height: 26px }
