.tianjia>input:nth-of-type(3){width: 76px;margin-left:15px }
.tianjia>input:nth-of-type(4){width: 102px;margin-left: 25px}
.tbes{margin: 30px 0 0 32px;}
#t{font-weight: 600;font-size: 15px;border-color: #cccccc}
#trt{color: white;}
