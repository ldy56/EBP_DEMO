var t=document.getElementById("t");
function tianjia(m,j) {
    if(m.innerHTML=="-") {
        var deltrow=document.getElementById("td"+j);
        deltrow.innerHTML="";
        document.getElementById("jian"+j).innerHTML="+";
    }else{

        var deltrow=document.getElementById("td"+j);
        deltrow.innerHTML="<td colspan='4' height='80'><span>序号</span><span>描述</span><span>单价</span><span>购票张数</span><span>金额</span><br><br> <span>1.1</span><span>《总有刁民想害朕》</span><span>50元</span><span>6</span><span>300元</span></td>";
        document.getElementById("jian"+j).innerHTML="-";
    }
}
